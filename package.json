{
    "name": "Delicious Fruit API",
    "version": "1.0.0-rc.0",
    "description": "The API you should use instead of throwing your monitor out the window",
    "engines": {
        "node": ">=16"
    },
    "main": "server.js",
    "scripts": {
        "dev": "ts-node-dev --inspect --poll --respawn --transpileOnly ./src/server.ts",
        "start": "ts-node-transpile-only ./src/server.ts",
        "start2": "node --inspect -r ts-node/register/transpile-only ./src/server.ts",
        "test": "jest --forceExit --detectOpenHandles ./tests/ --silent=false",
        "test-int": "jest --forceExit --detectOpenHandles --silent=false --bail=5 --notify --runTestsByPath=./tests/integration/*.test.ts ",
        "test-int-on-ci": "jest --forceExit --detectOpenHandles --silent=false --bail=5 --ci --runTestsByPath=./tests/integration/*.test.ts ",
        "tsc": "tsc",
        "start-plain": "node build/server.js",
        "docker-reboot": "docker build -t delfruit-server . && docker kill df-server & docker rm df-server & docker run -d --name df-server --network df-network -p 4201:4201 -v D:\\Projects\\delfruit2-server\\config:/home/node/app/config delfruit-server",
        "tsoa": "tsoa",
        "build-routes": "tsoa routes",
        "build-openapi-spec": "tsoa spec"
    },
    "author": "klazen108",
    "license": "ISC",
